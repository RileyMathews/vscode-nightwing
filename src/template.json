{
  "name": "{{name}}",
  "type": "{{type}}",
  "tokenColors": [
    {
      "name": "Keyword*",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "{{keyword}}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String*",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "{{string}}"
      }
    },
    {
      "name": "Punctuation*",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "{{punctuation}}"
      }
    },
    {
      "name": "Entity*",
      "scope": [
        "entity",
        "support"
      ],
      "settings": {
        "foreground": "{{entity}}"
      }
    },
    {
      "name": "Constant*",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "{{constant}}"
      }
    },
    {
      "name": "Comment*",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "{{string}}90"
      }
    }
  ],
  "colors": {
    "contrastBorder": "{{ui.border}}",
    "focusBorder": "{{ui.accentBorder}}",
    "foreground": "{{ui.foreground}}",
    "widget.shadow": "{{ui.background}}50",
    "selection.background": "{{ui.foreground}}70",
    "descriptionForeground": "{{ui.foreground}}",
    "errorForeground": "{{ui.foreground}}",
    "button.background": "{{ui.primary}}",
    "button.hoverBackground": "{{ui.primary}}30",
    "button.foreground": "{{ui.primaryContrast}}",
    "dropdown.background": "{{ui.background}}",
    "dropdown.listBackground": "{{ui.background}}",
    "dropdown.foreground": "{{ui.foreground}}",
    "dropdown.border": "{{ui.border}}",
    "input.background": "{{ui.accentBackground}}",
    "input.border": "{{ui.border}}",
    "input.foreground": "{{ui.accentForeground}}",
    "input.placeholderForeground": "{{ui.foreground}}",
    "inputOption.activeBorder": "{{ui.accentBorder}}",
    "inputValidation.errorBackground": "{{ui.error}}",
    "inputValidation.errorForeground": "{{ui.foreground}}",
    "inputValidation.errorBorder": "{{ui.accentBorder}}",
    "inputValidation.infoBackground": "{{ui.background}}",
