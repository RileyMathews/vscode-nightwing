{
  "name": "{{name}}",
  "type": "{{type}}",
  "tokenColors": [
    {
      "name": "Keyword*",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "{{red}}",
        "fontStyel": "italic"
      }
    },
    {
      "name": "Keyword.Operator*",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "{{purple}}"
      }
    },
    {
      "name": "String*",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "{{green}}"
      }
    },
    {
      "name": "Punctuation*",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "{{cyan}}"
      }
    },
    {
      "name": "Entity*",
      "scope": [
        "entity"
      ],
      "settings": {
        "foreground": "{{yellow}}"
      }
    },
    {
      "name": "Constant*",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "{{orange}}"
      }
    },
    {
      "name": "Comment*",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "{{green}}50"
      }
    },
    {
      "name": "Support*",
      "scope": [
        "support"
      ],
      "settings": {
        "foreground": "{{yellow}}"
      }
    }
  ],
