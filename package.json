{
    "name": "nightwing",
    "displayName": "Nightwing",
    "description": "Yet another dark theme.",
    "publisher": "rileymathews",
    "version": "0.1.3",
    "repository": {
        "type": "git",
        "url": "https://github.com/RileyMathews/vscode-nightwing"
    },
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Themes"
    ],
    "scripts": {
        "build": "mustache ./src/values.json ./src/template.json > ./themes/Nightwing-color-theme.json"
    },
    "contributes": {
        "themes": [
            {
                "label": "Nightwing",
                "uiTheme": "vs-dark",
                "path": "./themes/Nightwing-color-theme.json"
            }
        ]
    },
    "devDependencies": {
        "mustache": "^3.0.1"
    }
}
